    "dotenv": "^16.3.1",
    "esbuild": "^0.19.5",
    "graphql-compose-mongoose": "^9.8.0",
    "mongoose": "^7.6.3",
    "twilio": "^4.19.0"
  },
  "devDependencies": {
    "esbuild-register": "^3.5.0"
  }
}
